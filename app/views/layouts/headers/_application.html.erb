<header id="header-frame">
  <div class="content-wrap">
    <%= image_tag "home/welcome/close.svg", id: "close-header" %>

    <ul data-gumshoe>
      <li><%= link_to "home", home_url(subdomain: false) %></li>
      <li><%= link_to "/vision", vision_url(subdomain: false), class: active_class("/vision") %></li>
      <li><%= link_to "/bénévole", volunteers_url(subdomain: false), class: active_class("/volunteers") %></li>
      <li><%= link_to "/RJ2021", url_for("/2021"), class: active_class("/2021") %></li>
      <li><%= link_to "/RJLogin", login_url(subdomain: false), class: active_class("/login") %></li>
      <li><%= link_to "/FAQ", support_url(subdomain: false), class: active_class("/support") %></li>
    </ul>

    <%= render "layouts/headers/signin" %>
  </div>
</header>
